# frozen_string_literal: true

start_swo_docker_image(
  image_name: 'better_izivi_frontend_develop',
  labels: %w[
    traefik.backend=better-izivi-develop
    traefik.frontend.rule=Host:izivi-develop.stiftungswo.ch
  ]
)

start_swo_docker_image(
  image_name: 'better_izivi_api_develop',
  env_file: "#{site_directory}/.env",
  labels: %w[
    traefik.backend=better-izivi-api-develop
    traefik.frontend.rule=Host:izivi-api-develop.stiftungswo.ch
  ]
)
